{
    "model_type": "diffusion_cond",
    "sample_size": 2097152,
    "sample_rate": 44100,
    "audio_channels": 1,
    "model": {
        "pretransform": {
            "type": "autoencoder",
            "config": {
                "encoder": {
                    "type": "dac",
                    "config": {
                        "latent_dim": 64,
                        "d_model": 128,
                        "strides": [4, 8, 8, 8]
                    }
                },
                "decoder": {
                    "type": "dac",
                    "config": {
                        "latent_dim": 32,
                        "channels": 1536,
                        "rates": [8, 8, 8, 4]
                    }
                },
                "bottleneck": {
                    "type": "vae"
                },
                "latent_dim": 32,
                "downsampling_ratio": 2048,
                "io_channels": 1
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "t5",
                    "config": {
                        "t5_model_name": "t5-base",
                        "max_length": 77
                    }
                }
            ],
            "cond_dim": 768
        },
        "diffusion": {
            "type": "adp_cfg_1d",
            "cross_attention_cond_ids": ["prompt"],
            "config": {
                "in_channels": 32,
                "context_embedding_features": 768,
                "context_embedding_max_length": 77,
                "channels": 256,
                "resnet_groups": 8,
                "kernel_multiplier_downsample": 2,
                "multipliers": [2, 3, 3, 4, 4],
                "factors": [1, 2, 2, 2],
                "num_blocks": [2, 2, 2, 2],
                "attentions": [0, 3, 3, 3, 3],
                "attention_heads": 16,
                "attention_multiplier": 2,
                "use_nearest_upsample": false,
                "use_skip_scale": true,
                "use_context_time": true
            }
        },
        "io_channels": 32
    },
    "training": {
        "learning_rate": 1e-4,
        "demo": {
            "demo_every": 2000,
            "demo_steps": 250,
            "num_demos": 4,
            "demo_cond": [
                {"prompt": "Style: Groovy|Key: G|Instruments: Acoustic Guitar|Genre: Folk|Instrument: Guitalele|Mood: Silly|Tempo: 120|Year: 2017|Beard: Shadow"},
                {"prompt": "Date: 2012-07-15|Tempo: 70|Key: F#|Mood: Tired|Year: 2012|Beard: Shadow|Genre: Folk|Style: Rambling, Rough"},
                {"prompt": "Tempo: 100|Genre: Rock|Style: Rambling, Fun, Bouncy|Mood: Pensive|Key: E|Year: 2021|Topic: Poetic|Beard: Shadow"},
                {"prompt": "Tempo: 150|Genre: Folk|Style: Happy, Rambling"}
            ],
            "demo_cfg_scales": [3, 6, 9]
        }
    }
}