{
    "model_type": "diffusion_cond",
    "sample_size": 262144,
    "sample_rate": 48000,
    "audio_channels": 2,
    "model": {
        "pretransform": {
            "type": "autoencoder",
            "config": {
                "encoder": {
                    "type": "seanet",
                    "config": {
                        "channels": 2,
                        "dimension": 32,
                        "n_filters": 128,
                        "ratios": [2, 2, 2, 4],
                        "n_residual_layers": 3,
                        "dilation_base": 3,
                        "norm": "none",
                        "lstm": 0
                    }
                },
                "decoder": {
                    "type": "seanet",
                    "config": {
                        "channels": 2,
                        "dimension": 32,
                        "n_filters": 128,
                        "ratios": [2, 2, 2, 4],
                        "n_residual_layers": 3,
                        "dilation_base": 3,
                        "norm": "none",
                        "lstm": 0
                    }
                },
                "bottleneck": {
                    "type": "tanh"
                },
                "latent_dim": 32,
                "downsampling_ratio": 32,
                "io_channels": 2
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "t5",
                    "config": {
                        "t5_model_name": "t5-base",
                        "max_length": 77
                    }
                }
            ],
            "cond_dim": 768
        },
        "diffusion": {
            "type": "adp_cfg_1d",
            "cross_attention_cond_ids": ["prompt"],
            "config": {
                "in_channels": 32,
                "context_embedding_features": 768,
                "context_embedding_max_length": 77,
                "channels": 256,
                "resnet_groups": 8,
                "kernel_multiplier_downsample": 2,
                "multipliers": [2, 3, 3, 3, 4, 4],
                "factors": [1, 2, 2, 2, 2],
                "num_blocks": [2, 2, 2, 2, 2],
                "attentions": [0, 3, 3, 3, 3, 3],
                "attention_heads": 16,
                "attention_multiplier": 2,
                "use_nearest_upsample": false,
                "use_skip_scale": true,
                "use_context_time": true
            }
        },
        "io_channels": 32
    },
    "demo": {
        "num_demos": 8,
        "demo_cond": [
            {"prompt": "Amen break 174 BPM"},
            {"prompt": "A car honking on a busy street"},
            {"prompt": "People talking in a crowded cafe"},
            {"prompt": "A short, beautiful piano riff in C minor"},
            {"prompt": "Tight Snare Drum"},
            {"prompt": "Calm, meditative ambient drone"},
            {"prompt": "Rattling snare"},
            {"prompt": "Clean bright guitar loop"}
        ],
        "demo_cfg_scales": [3, 5, 7]
    }
}