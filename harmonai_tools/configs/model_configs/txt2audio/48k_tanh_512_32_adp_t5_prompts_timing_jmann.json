            {"prompt": "Date: 2012-07-15|Tempo: 70|Key: F#|Mood: Tired|Year: 2012|Beard: Shadow|Genre: Folk|Style: Rambling, Rough", "seconds_start": 0, "seconds_total": 140},
            {"prompt": "Tempo: 100|Genre: Rock|Style: Rambling, Fun, Bouncy|Mood: Pensive|Key: E|Year: 2021|Topic: Poetic|Beard: Shadow", "seconds_start": 0, "seconds_total": 150},
            {"prompt": "Tempo: 150|Genre: Folk|Style: Happy, Rambling", "seconds_start": 0, "seconds_total": 202}
