{
    "model_type": "diffusion_cond",
    "sample_size": 2097152,
    "sample_rate": 48000,
    "audio_channels": 1,
    "model": {
        "pretransform": {
            "type": "autoencoder",
            "config": {
                "encoder": {
                    "type": "dac",
                    "config": {
                        "latent_dim": 32,
                        "d_model": 128,
                        "strides": [2, 4, 8, 8]
                    }
                },
                "decoder": {
                    "type": "dac",
                    "config": {
                        "latent_dim": 32,
                        "channels": 1536,
                        "rates": [8, 8, 4, 2]
                    }
                },
                "bottleneck": {
                    "type": "tanh"
                },
                "latent_dim": 32,
                "downsampling_ratio": 512,
                "io_channels": 1
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "t5",
                    "config": {
                        "t5_model_name": "t5-base",
                        "max_length": 77
                    }
                },
                {
                    "id": "seconds_start",
                    "type": "int",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                },
                {
                    "id": "seconds_total",
                    "type": "int",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                }
            ],
            "cond_dim": 768
        },
        "diffusion": {
            "type": "adp_cfg_1d",
            "cross_attention_cond_ids": ["prompt", "seconds_start", "seconds_total"],
            "config": {
                "in_channels": 32,
                "context_embedding_features": 768,
                "context_embedding_max_length": 80,
                "channels": 256,
                "resnet_groups": 16,
                "kernel_multiplier_downsample": 2,
                "multipliers": [2, 2, 3, 3, 3],
                "factors": [1, 2, 2, 4],
                "num_blocks": [3, 3, 3, 3],
                "attentions": [0, 0, 3, 3, 3],
                "attention_heads": 8,
                "attention_features": 64,
                "attention_multiplier": 4,
                "use_nearest_upsample": false,
                "use_skip_scale": true,
                "use_context_time": true
            }
        },
        "io_channels": 32
    },
    "demo": {
        "num_demos": 4,
        "demo_cond": [
