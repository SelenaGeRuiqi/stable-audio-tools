            "config": {
                "in_channels": 128,
                "context_embedding_features": 768,
                "context_embedding_max_length": 128,
                "channels": 256,
                "resnet_groups": 16,
                "kernel_multiplier_downsample": 2,
                "multipliers": [2, 3, 3, 4, 4, 5],
                "factors": [1, 2, 3, 5, 5],
                "num_blocks": [3, 3, 3, 3, 3],
                "attentions": [1, 1, 1, 1, 1, 1],
                "attention_heads": 16,
                "attention_features": 64,
                "attention_multiplier": 4,
                "attention_use_rel_pos": false,
                "use_nearest_upsample": false,
                "use_skip_scale": true,
                "use_context_time": true
            }
        },
        "io_channels": 128
    },
    "training": {
        "learning_rate": 1e-4,
        "demo": {
            "demo_every": 2000,
            "demo_steps": 250,
            "num_demos": 4,
