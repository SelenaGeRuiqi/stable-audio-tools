{
    "model_type": "diffusion_cond",
    "sample_size": 1048576,
    "sample_rate": 44100,
    "audio_channels": 1,
    "model": {
        "pretransform": {
            "type": "autoencoder",
            "config": {
                "encoder": {
                    "type": "dac",
                    "config": {
                        "latent_dim": 64,
                        "d_model": 128,
                        "strides": [4, 8, 8, 8]
                    }
                },
                "decoder": {
                    "type": "dac",
                    "config": {
                        "latent_dim": 32,
                        "channels": 1536,
                        "rates": [8, 8, 8, 4]
                    }
                },
                "bottleneck": {
                    "type": "vae"
                },
                "latent_dim": 32,
                "downsampling_ratio": 2048,
                "io_channels": 1
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "t5",
                    "config": {
                        "t5_model_name": "t5-base",
                        "max_length": 77
                    }
                },
                {
                    "id": "seconds_start",
                    "type": "int",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                },
                {
                    "id": "seconds_total",
                    "type": "int",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                }
            ],
            "cond_dim": 768
        },
        "diffusion": {
            "type": "adp_cfg_1d",
            "cross_attention_cond_ids": ["prompt", "seconds_start", "seconds_total"],
            "config": {
                "in_channels": 32,
                "context_embedding_features": 768,
                "context_embedding_max_length": 80,
                "channels": 256,
                "resnet_groups": 8,
                "kernel_multiplier_downsample": 2,
                "multipliers": [2, 3, 3, 4, 4, 5],
                "factors": [1, 2, 4, 4, 4],
                "num_blocks": [2, 2, 2, 2, 2],
                "attentions": [0, 0, 3, 3, 3, 3],
                "attention_heads": 16,
                "attention_features": 64,
                "attention_multiplier": 4,
                "use_nearest_upsample": false,
                "use_skip_scale": true,
                "use_context_time": true
            }
        },
        "io_channels": 32
    },
    "demo": {
        "num_demos": 4,
        "demo_cond": [
            {"prompt": "A slow, sad piano piece that will make you cry. Perfect for a sad scene in a movie.", "seconds_start": 0, "seconds_total": 45},
            {"prompt": "80s electronic track with melodic synthesizers, catchy beat and groovy bass", "seconds_start": 0, "seconds_total": 120},
            {"prompt": "A grand orchestral arrangement with thunderous percussion, epic brass fanfares, and soaring strings, creating a cinematic atmosphere fit for a heroic battle.", "seconds_start": 0, "seconds_total": 25},
            {"prompt": "tech house track, four on the floor, great track to play in the club", "seconds_start": 0, "seconds_total": 60}   
        ],
        "demo_cfg_scales": [3, 6, 9]
    }
}