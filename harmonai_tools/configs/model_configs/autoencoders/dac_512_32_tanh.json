{
    "model_type": "autoencoder",
    "sample_size": 32768,
    "sample_rate": 32000,
    "audio_channels": 1,
    "model": {
        "encoder": {
            "type": "dac",
            "config": {
                "latent_dim": 32,
                "d_model": 128,
                "strides": [2, 4, 8, 8]
            }
        },
        "decoder": {
            "type": "dac",
            "config": {
                "latent_dim": 32,
                "channels": 1536,
                "rates": [8, 8, 4, 2]
            }
        },
        "bottleneck": {
            "type": "tanh"
        },
        "latent_dim": 32,
        "downsampling_ratio": 512,
        "io_channels": 1
    },
    "training": {
        "batch_size": 8,
        "learning_rate": 1e-4,
        "loss_configs": {
            "discriminator": {
                "type": "encodec",
                "config": {
                    "filters": 32,
                    "in_channels": 1,
                    "n_ffts": [2048, 1024, 512, 256, 128, 64, 32],
                    "hop_lengths": [512, 256, 128, 64, 32, 16, 8],
                    "win_lengths": [2048, 1024, 512, 256, 128, 64, 32]
                },
                "weights": {
                    "adversarial": 0.1,
                    "feature_matching": 5.0
                }
            },
            "spectral": {
                "type": "mrstft",
                "config": {
                    "fft_sizes": [2048, 1024, 512, 256, 128, 64, 32],
                    "hop_lengths": [512, 256, 128, 64, 32, 16, 8],
                    "win_lengths": [2048, 1024, 512, 256, 128, 64, 32],
                    "perceptual_weighting": true
                }
            }
        },
        "epochs": 1000,
        "save_checkpoint_interval": 1000
    }
}