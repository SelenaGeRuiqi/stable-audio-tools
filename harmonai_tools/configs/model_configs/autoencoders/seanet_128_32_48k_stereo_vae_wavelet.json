{
    "model_type": "autoencoder",
    "sample_size": 32768,
    "sample_rate": 48000,
    "audio_channels": 2,
    "model": {
        "pretransform":{
            "type": "wavelet",
            "enable_grad": true,
            "config": {
                "levels": 3,
                "wavelet": "bior4.4",
                "channels": 2
            }
        },
        "encoder": {
            "type": "seanet",
            "config": {
                "channels": 16,
                "dimension": 64,
                "n_filters": 128,
                "ratios": [2, 2, 4],
                "n_residual_layers": 3,
                "dilation_base": 3,
                "norm": "weight_norm"
            }
        },
        "decoder": {
            "type": "seanet",
            "config": {
                "channels": 16,
                "dimension": 32,
                "n_filters": 128,
                "ratios": [2, 2, 4],
                "n_residual_layers": 3,
                "dilation_base": 3,
                "norm": "weight_norm"
            }
        },
        "bottleneck": {
            "type": "vae"
        },
        "latent_dim": 32,
        "downsampling_ratio": 128,
        "io_channels": 2
    },
    "training": {
        "learning_rate": 4e-5,
        "warmup_steps": 0,
        "use_ema": true,
        "loss_configs": {
            "discriminator": {
                "type": "encodec",
                "config": {
                    "filters": 32,
                    "n_ffts": [2048, 1024, 512, 256, 128],
                    "hop_lengths": [512, 256, 128, 64, 32],
                    "win_lengths": [2048, 1024, 512, 256, 128]
                },
                "weights": {
                    "adversarial": 0.1,
                    "feature_matching": 5.0
                }
            },
            "spectral": {
                "type": "mrstft",
                "config": {
                    "fft_sizes": [2048, 1024, 512, 256, 128, 64, 32],
                    "hop_sizes": [512, 256, 128, 64, 32, 16, 8],
                    "win_lengths": [2048, 1024, 512, 256, 128, 64, 32],
                    "perceptual_weighting": true
                },
                "weights": {
                    "mrstft": 1.0
                }
            },
            "time": {
                "type": "l1",
                "weights": {
                    "l1": 0.0
                }
            },
            "bottleneck": {
                "type": "kl",
                "weights": {
                    "kl": 1e-4
                }
            }
        },
        "demo": {
            "demo_every": 2000
        }
    }
}