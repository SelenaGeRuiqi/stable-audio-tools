{
    "model_type": "autoencoder",
    "sample_size": 65536,
    "sample_rate": 48000,
    "audio_channels": 2,
    "model": {
        "encoder": {
            "type": "seanet",
            "config": {
                "channels": 2,
                "dimension": 32,
                "n_filters": 128,
                "ratios": [2, 2, 2, 4],
                "n_residual_layers": 3,
                "dilation_base": 3,
                "norm": "none",
                "lstm": 0
            }
        },
        "decoder": {
            "type": "seanet",
            "config": {
                "channels": 2,
                "dimension": 32,
                "n_filters": 128,
                "ratios": [2, 2, 2, 4],
                "n_residual_layers": 3,
                "dilation_base": 3,
                "norm": "none",
                "lstm": 0
            }
        },
        "bottleneck": {
            "type": "tanh"
        },
        "latent_dim": 32,
        "downsampling_ratio": 32,
        "io_channels": 2
    },
    "training": {
        "learning_rate": 4e-5,
        "warmup_steps": 100000,
        "use_ema": true,
        "loss_configs": {
            "discriminator": {
                "type": "encodec",
                "config": {
                    "filters": 32,
                    "n_ffts": [2048, 1024, 512, 256, 128],
                    "hop_lengths": [512, 256, 128, 64, 32],
                    "win_lengths": [2048, 1024, 512, 256, 128]
                },
                "weights": {
                    "adversarial": 0.1,
                    "feature_matching": 2.0
                }
            },
            "spectral": {
                "type": "mrstft",
                "config": {
                    "fft_sizes": [2048, 1024, 512, 256, 128, 64, 32],
                    "hop_sizes": [512, 256, 128, 64, 32, 16, 8],
                    "win_lengths": [2048, 1024, 512, 256, 128, 64, 32],
                    "perceptual_weighting": true
                },
                "weights": {
                    "mrstft": 1.0
                }
            },
            "time": {
                "type": "l1",
                "weights": {
                    "l1": 0.0
                }
            }
        },
        "demo": {
            "demo_every": 2000
        }
    }
}